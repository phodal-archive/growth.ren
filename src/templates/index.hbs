<div class="row medium-12 large-12 columns">
  {{#each  articles }}
      <article class="blog-post" itemscope itemprop="blogPost">
          <div class="article-inner">
              <header class="article-header">
                  <h1 itemprop="name">
                      <a class="article-title" href="/blog/{{url }}.html">{{title}}</a><small><time datetime="{{date }}" itemprop="datePublished">{{date }}</time></small>
                  </h1>
              </header>
              <div class="article-entry" itemprop="articleBody">
                {{#markdown}}
                  {{description }}
                {{/markdown}}
              </div>
              <footer class="article-footer">
              </footer>
          </div>
      </article>
  {{/each}}
</div>
